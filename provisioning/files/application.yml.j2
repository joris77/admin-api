spring:
  jpa:
    database: POSTGRESQL
    show-sql: false
    hibernate.ddl-auto: none

  database:
    driverClassName: org.postgresql.Driver

  datasource:
    platform: postgres
    username: admin-api
    password: {{lookup('password', 'credentials/admin_api.txt')}}
    url: jdbc:postgresql://{{ db_address }}:5432/admin

liquibase:
  user: admin-owner
  password: {{lookup('password', 'credentials/admin_owner.txt')}}
  url: jdbc:postgresql://{{ db_address }}:5432/admin

cross-origin-addresses: {{ cross_origin_addresses }}

logging:
  level:
    org.springframework.web: DEBUG